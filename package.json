{
  "publisher": "rickytjx",
  "name": "open-in-coding-button",
  "displayName": "Open in CODING Button",
  "version": "0.1.3",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "Add a button to go to the CODING on the status bar",
  "author": "Ricky Tang <wbzhzd@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rickytjx/vscode-open-in-coding-button#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rickytjx/vscode-open-in-coding-button"
  },
  "bugs": {
    "url": "https://github.com/rickytjx/vscode-open-in-coding-button/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.78.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "icons": {
      "coding-logo": {
        "description": "CODING LOGO icon",
        "default": {
          "fontPath": "res/coding-fonts.woff",
          "fontCharacter": "\\e6ba"
        }
      }
    },
    "configuration": {
      "type": "object",
      "title": "Open in CODING - Configuration",
      "properties": {
        "openInCODING.coding.domain": {
          "type": "string",
          "description": "Custom CODING domain",
          "default": "e.coding.net"
        },
        "openInCODING.remote.name": {
          "type": "string",
          "description": "Name of the remote repository",
          "default": "origin"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@antfu/ni": "^0.21.3",
    "@types/absolute": "^0.0.29",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.16",
    "@types/vscode": "^1.78.1",
    "@vscode/vsce": "^2.19.0",
    "absolute": "^0.0.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.42.0",
    "esno": "^0.16.3",
    "find-up": "^6.3.0",
    "lodash-es": "^4.17.21",
    "pify": "^6.1.0",
    "pnpm": "^8.6.0",
    "rimraf": "^5.0.1",
    "simple-git": "^3.19.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.3",
    "vitest": "^0.31.4"
  }
}
